<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ClassifyView">
        <attr name="SubRatio" format="fraction"/>
        <attr name="MainSpanCount" format="integer"/>
        <attr name="SubSpanCount" format="integer"/>
        <attr name="ShadowColor" format="color"/>
        <attr name="AnimationDuration" format="integer"/>
        <attr name="EdgeWidth" format="dimension"/>
        <attr name="MainPaddingLeft" format="dimension"/>
        <attr name="MainPaddingTop" format="dimension"/>
        <attr name="MainPaddingRight" format="dimension"/>
        <attr name="MainPaddingBottom" format="dimension"/>
        <attr name="MainPadding" format="dimension"/>
        <attr name="MainClipToPadding" format="boolean"/>
        <attr name="SubPaddingLeft" format="dimension"/>
        <attr name="SubPaddingTop" format="dimension"/>
        <attr name="SubPaddingRight" format="dimension"/>
        <attr name="SubPaddingBottom" format="dimension"/>
        <attr name="SubPadding" format="dimension"/>
        <attr name="SubClipToPadding" format="boolean"/>
        <attr name="DragScaleX" format="float"/>
        <attr name="DragScaleY" format="float"/>
        <attr name="DragInMergeScaleX" format="float"/>
        <attr name="DragInMergeScaleY" format="float"/>
        <attr name="DragScalePivotGravity" format="enum">
            <enum name="leftTop" value="0"/>
            <enum name="rightTop" value="1"/>
            <enum name="leftBottom" value="2"/>
            <enum name="rightBottom" value="3"/>
            <enum name="center" value="4"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="InsertAbleGridView">
        <attr name="RowCount" format="integer"/>
        <attr name="ColumnCount" format="integer"/>
        <attr name="RowGap" format="dimension"/>
        <attr name="ColumnGap" format="dimension"/>
        <attr name="InnerPadding" format="dimension"/>
        <attr name="OutlinePadding" format="dimension"/>
        <attr name="OutlineWidth" format="dimension"/>
        <attr name="OutlineColor" format="color"/>
    </declare-styleable>
</resources>